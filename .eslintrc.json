{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
		"camelcase": "off",
		"arrow-parens": 0,
		"no-unused-vars": 0,
		"import/no-unresolved": 0,
		"no-empty-function": "off",
		"class-methods-use-this": "off",
		"import/prefer-default-export": "off",
		"no-shadow": "off",
		"no-console": "off",
		"no-useless-constructor": "off",
		"import/extensions": 0,
		"max-len": 0,
        "import/order": 0
	}
}